#include<iostream>
using namespace std;
class Addition{
int s,sum1;
public :
    void sum(int x,int y)
    {
        s=x+y;
        cout <<"two integer sum"<<s;
        }

    void sum(int x,int y ,int z){
        sum1=x+y+z;
        cout <<"three integer sum"<<sum1;
    }
    } ;
int main(){
Addition a;
a.sum(2,3);
a.sum(2,4,5);
}
